<mapper namespace="com.readboy.ssm.mapper.PerformanceGzhzMapper">

 	<select id="findPerformanceGzhz" resultType="PerformanceGzhz">
 		select * from v_wage_hz where yggh = #{yggh} and
 		gzyf between #{ksrq} and #{jsrq}
 	</select>
 	
 </mapper>
