<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.readboy.ssm.mapper.ResMapper">
 	
 	<select id="findRes" resultType="Res">
 		select * from res_path
 	</select>
 	
 	<update id="updateRes" >
 		update res_path set res_path.path = #{path}
 	</update>
 	
 	
 	<insert id="insertRes" >
 		insert into res_path set path = #{path}
 	</insert>
 </mapper>
